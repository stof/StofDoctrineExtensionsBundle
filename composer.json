{
    "name": "stof/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "Integration of the gedmo/doctrine-extensions with Symfony2",
    "keywords": ["tree", "behaviors", "doctrine2", "extensions", "gedmo", "sluggable","loggable", "translatable", "nestedset", "sortable", "timestampable"],
    "homepage": "https://github.com/stof/StofDoctrineExtensionsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.1.*",
        "gedmo/doctrine-extensions": ">=2.3-dev"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "*",
        "doctrine/mongodb-odm-bundle": "2.1.*"
    },
    "autoload": {
        "psr-0": { "Stof\\DoctrineExtensionsBundle": "" }
    },
    "target-dir": "Stof/DoctrineExtensionsBundle"
}
